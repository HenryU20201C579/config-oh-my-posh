{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "üßëüèª {{ .UserName }}",
          "foreground": "#ffffff",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "dallas",
            "threshold": 0
          },
          "template": " \u231b{{ .FormattedMs }}s ",
          "foreground": "#94ffa2",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "root_icon": "\uf292"
          },
          "template": "\uf0e7",
          "type": "root",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "üíªCPU:</>{{ round .PhysicalPercentUsed .Precision }}% ",
          "foreground": "#94ffa2",
          "type": "sysinfo",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#ffffff>\ue266</> RAM:{{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1000000000.0) }}GB ",
          "foreground": "#94ffa2",
          "type": "sysinfo",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "time_format": " 3:04:05 PM"
          },
          "template": "üìÖ{{ .CurrentDate | date .Format }} ",
          "foreground": "#7eb8da",
          "type": "time",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " <#ffffff>{{ .UpstreamIcon }}</>{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <#ffffff>\uf044</> {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#ffffff>\uf046</> {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} <#ffffff>\ueb4b</> {{ .StashCount }}{{ end }} ",
          "foreground": "#ff8800",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .Profile }}{{if .Region}}@{{ .Region }}{{ end }} ",
          "foreground": "#ff8800",
          "type": "aws",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} ",
          "foreground": "#ff8800",
          "type": "kubectl",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "[\uf0e7]",
          "foreground": "#ff8800",
          "type": "root",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": "\u2516 <#98bfad>{{ .Path }}</>",
          "foreground": "#7eb8da",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " <#ffffff>\uea6c</> Error",
          "foreground": "#ff8800",
          "type": "exit",
          "style": "powerline"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2514\u2500 üí¨",
          "foreground": "#7eb8da",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
